doctype html
html(lang='en')
  head
    title flow.ci - eslint format renderer
    link(rel='stylesheet', href='https://cdnjs.cloudflare.com/ajax/libs/normalize/4.0.0/normalize.min.css')
    link(rel='stylesheet', href='https://fonts.useso.com/css?family=Open+Sans:400,300,600')
    style.
      body{
        font-family: 'Open Sans';
      }
  body
    h1 Format Renderer for eslint
    
    ul
      each file in files
        li= file.filePath
